<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog
        id="visitorDialog"
        title="{visitors>name}"
        draggable="true"
        resizable="true">
        <content>
            <VBox>
                <Text text="Visitor Details" />
                <Text text="Email: {email}" />
                <Text text="Phone: {phone}" />
                <Text text="Company: {company}"/>
            </VBox>
        </content>
        <buttons>
            <Button text="Close" press="onCloseVisitorDialog" />
        </buttons>
    </Dialog>
</core:FragmentDefinition>
