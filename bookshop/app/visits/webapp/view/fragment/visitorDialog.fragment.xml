<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Dialog id="visitorDialog" title="Visitor Details">
    <content>
      <VBox>
        <Label text="Email:" />
        <Text text="{visitors/email}" />
        <Label text="Phone:" />
        <Text text="{visitors/phone}" />
        <Label text="Company:" />
        <Text text="{visitors/company}" />
      </VBox>
    </content>
    <beginButton>
      <Button text="Close" press="onCloseDialog" />
    </beginButton>
  </Dialog>
</core:FragmentDefinition>

