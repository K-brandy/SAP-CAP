<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<SelectDialog 
     id="idSelectDialog"
     noDataText="Visitor not Found"
     title="Select Visitor"
     search="onSelectDialogSearch"
     confirm="onVisitorsSelectDialogConfirm"
     cancel="onSelectDialogCancel"
    
     items="{
           path:'/Visitors'  
           }"
>
<StandardListItem 
       title="{name}"
       description="{= ${ID} + ' - ' + ${country} + ','+ ${street}+','+${postal_code}}"
       type="Active"
/>
</SelectDialog>
</c:FragmentDefinition>